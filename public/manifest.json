{
  "manifest_version": 2,
  "name": "React Chrome Extension",
  "version": "1",
  "browser_action": {
    "default_icon": "logo192.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ],
  "permissions": ["tabs", "bookmarks"]
}
